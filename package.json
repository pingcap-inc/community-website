{
  "name": "@pingcap/tug-website",
  "private": true,
  "scripts": {
    "build": "next build && npm run build:asktug",
    "build:accounts": "lerna run --scope @tidb-community/accounts build",
    "build:accounts:staging": "lerna run --scope @tidb-community/accounts build:staging",
    "build:asktug": "lerna run --scope @tidb-community/asktug build",
    "build:shared": "lerna run --ignore @tidb-community/accounts --ignore @tidb-community/asktug build",
    "build:ui": "lerna run --scope @tidb-community/ui build",
    "build:common": "lerna run --scope @tidb-community/common build",
    "build:watch": "lerna run --no-bail build:watch",
    "deploy": "shipit production deploy",
    "dev": "next dev",
    "lerna:bootstrap": "lerna bootstrap",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "postinstall": "npm run lerna:bootstrap && npm run build:shared",
    "server:init": "next build && npm run server:start",
    "server:reload": "pm2 reload pm2.config.js",
    "server:start": "pm2 start pm2.config.js",
    "start": "next start",
    "start:accounts": "lerna run --scope @tidb-community/accounts start",
    "start:mockApi": "lerna run --scope @tidb-community/mock-api start",
    "storybook": "lerna run --scope @tidb-community/ui storybook -- -- -s $(pwd)/public",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "engines": {
    "node": "14.13.0",
    "npm": "6.14.8"
  },
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@sentry/nextjs": "6.4.1",
    "ahooks": "2.10.4",
    "antd": "4.15.6",
    "axios": "0.21.1",
    "classnames": "2.3.1",
    "dayjs": "1.10.4",
    "formik": "2.2.8",
    "formik-antd": "2.0.1",
    "next": "10.2.2",
    "next-i18next": "8.5.0",
    "polished": "4.1.2",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "styled-components": "5.3.0",
    "swr": "0.5.6",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "1.15.1",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@types/faker": "5.5.5",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.27.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "i18next-http-backend": "1.2.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lerna": "4.0.0",
    "lint-staged": "10.5.4",
    "pm2": "4.5.6",
    "prettier": "2.3.0",
    "react-test-renderer": "17.0.2",
    "sass": "1.34.0",
    "shipit-cli": "5.3.0",
    "shipit-deploy": "5.3.0"
  }
}
