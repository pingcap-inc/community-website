{
  "name": "@pingcap/tug-website",
  "private": true,
  "scripts": {
    "build": "next build --no-lint",
    "build:preview": "cat .env.preview > .env.production && npm run build",
    "build:production": "npm run build",
    "build:watch": "lerna run --no-bail build:watch",
    "copy:locales": "rm -rf public/locales && cp -r packages/i18n/locales public",
    "deploy:production": "shipit production deploy",
    "deploy:preview": "shipit preview deploy",
    "dev": "npm run start:i18n && npm run next:dev",
    "lint": "next lint && eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "lerna bootstrap",
    "prepare": "husky install",
    "server:init": "next build && npm run server:start",
    "server:reload": "pm2 reload pm2.config.js",
    "server:start": "pm2 start pm2.config.js",
    "start": "next start",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "start:i18n": "lerna run --scope @tidb-community/i18n start",
    "storybook": "lerna run --scope @tidb-community/ui storybook -- -- -s $(pwd)/public",
    "pretest": "cp babel.config.js.old babel.config.js",
    "posttest": "rm babel.config.js",
    "test": "jest",
    "test:changed": "npm test -- --changedSince=origin/main",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "remove:cache": "rm -rf ./packages/**/node_modules ./node_modules ./.next"
  },
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@formatjs/intl-numberformat": "7.2.3",
    "@fullcalendar/core": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@reduxjs/toolkit": "1.6.1",
    "@sentry/nextjs": "6.18.0",
    "ahooks": "2.10.9",
    "antd": "4.15.6",
    "antd-img-crop": "3.14.2",
    "axios": "^0.26.1",
    "classnames": "2.3.1",
    "dayjs": "1.10.6",
    "formik": "2.2.9",
    "formik-antd": "2.0.1",
    "js-convert-case": "4.1.2",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "moment": "2.29.1",
    "next": "12.1.0",
    "next-i18next": "8.7.0",
    "next-transpile-modules": "8.0.0",
    "polished": "4.1.3",
    "prop-types": "15.8.1",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-cool-dimensions": "2.0.7",
    "react-dom": "17.0.2",
    "react-infinite-scroll-component": "6.1.0",
    "react-redux": "7.2.5",
    "react-scroll": "1.8.4",
    "react-transition-group": "4.4.2",
    "styled-components": "5.3.1",
    "swr": "1.2.2",
    "tslib": "2.3.1",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.4",
    "@pingcap-inc/tidb-community-editor": "0.4.1",
    "@pingcap-inc/tidb-community-site-components": "1.1.7",
    "@pingcap-inc/tidb-community-ui": "1.1.0",
    "@sentry/webpack-plugin": "1.17.1",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/faker": "5.5.8",
    "@types/react-scroll": "1.8.3",
    "@types/styled-components": "5.1.22",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "babel-plugin-transform-import-ignore": "^1.1.0",
    "babel-preset-react-app": "10.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.1.0",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "micromatch": "4.0.4",
    "pm2": "5.1.1",
    "prettier": "2.3.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "sass": "1.39.0",
    "shipit-cli": "5.3.0",
    "shipit-deploy": "5.3.0",
    "svgo": "2.5.0",
    "typescript": "4.5.4"
  }
}
